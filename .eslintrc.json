{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:vue/recommended",
		"prettier"
	],
	"rules": {
		"react/prop-types": 0,
		"no-console": 1
	},
	"plugins": ["import"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
